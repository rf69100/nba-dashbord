{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "public": false,
  "env": {},
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "^/(?:.+/)?(?:index\\.html)?$",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|gif|ico|jpg|jpeg|jpg|png|svg|webp|woff|woff2|eot|otf|ttf)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
